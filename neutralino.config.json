{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/hhkb-react/build/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*",
    "os.*",
    "debug.log"
  ],
  "modes": {
    "window": {
      "title": "hhkb",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/hhkb-react/public/logo192.png",
      "enableInspector": false
    }
  },
  "cli": {
    "binaryName": "hhkb",
    "resourcesPath": "/hhkb-react/build/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/hhkb-react/public/neutralino.js",
    "binaryVersion": "4.7.0",
    "clientVersion": "3.6.0",
    "frontendLibrary": {
        "patchFile": "/hhkb-react/public/index.html",
        "devUrl": "http://localhost:3000"
    }
  }
}
